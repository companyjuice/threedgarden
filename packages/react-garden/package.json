  "private": false,
  "scripts": {
    "________0": "=== custom scripts ===============================",
    "mm": "next -p 7777",
    "mm:scrub": "rd /S /Q .\\node_modules & rd /S /Q .\\.next & del /F .\\pnpm-lock.yaml ;",
    "mm:install": "npm install --no-optional --omit=optional",
    "mm:install:full": "npm install",
    "mm:install:pnpm": "pnpm i",
    "________1": "=== use these ====================================",
    "next": "next -p 7777",
    "next:dev": "next dev -p 7777",
    "next:build": "next build",
    "next:start": "next start -p 7777",
    "next:test": "echo \"Error: no test specified\" && exit 1",
    "________2": "=== old, do not use these ========================",
    "old:start": "react-scripts start -p 2999",
    "old:build": "react-scripts build",
    "old:test": "react-scripts test",
    "old:eject": "react-scripts eject",
    "________3": "=== cleanup on aisle nine ========================",
    "prettier": "prettier --write **/*.{ts,tsx,js,jsx}",
    "prettier:eslint": "prettier-eslint --write **/*.{ts,tsx,js,jsx}",
    "lint": "eslint src --ext .ts",
    "lint:fix": "pnpm lint -- --fix",
    "lint:next": "next lint",
    "________4": "=== testing, testing =============================",
    "test:run": "jest",
    "test:lint": "run lint && pnpm test:run",
    "________X": "=================================================="
  },
