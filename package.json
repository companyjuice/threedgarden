  "scripts": {
    "___________________1": "____ active scripts ____________________________________",
    "i": "yarn install",
    "b": "yarn build",
    "s": "yarn start",
    "start": "next -p 7777",
    "dev": "next dev -p 7777",
    "build": "next build",
    "deploy": "next deploy",
    "export": "next export",
    "test": "next test",
    "lint": "next lint",
    "___________________2": "____ cleanup on aisle nine _____________________________",
    "prettier:mm": "prettier --write **/*.{ts,tsx,js,jsx}",
    "prettier:demo": "prettier --write ./public/demo/*.{ts,tsx,js,jsx}",
    "lint:es": "eslint src",
    "lint:demo": "eslint ./public/demo/*.{ts,tsx,js,jsx}",
    "lint:fix": "yarn lint -- --fix",
    "lint:next": "next lint",
    "lint:original": "eslint --config ./.eslintrc.js --ignore-path ./.eslintignore ./src",
    "___________________3": "____ use these: to delete build files __________________",
    "scrub:yarn": "rd /S /Q .\\node_modules & del /F .\\yarn-lock.yaml ;",
    "scrub:pnpm": "rd /S /Q .\\node_modules & del /F .\\pnpm-lock.yaml ;",
    "scrub:next": "rd /S /Q .\\.next & del /F .\\next-env.d.ts ;",
    "___________________4": "____ testing, testing __________________________________",
    "test:jest": "jest",
    "test:tsc": "tsc",
    "test:lint": "yarn lint && yarn test",
    "___________________X": "____ ___________________________________________________"
  },
