{
  "name": "threed-garden",
  "version": "0.12.0-alpha.0",
  "description": "ThreeD Garden is a full web3 app running on Three.js + React-Three-Fiber using NextJS/Vite/Vue and Farmbot Ruby/JS Web-App, available as a standalone app and a WordPress plugin. 'threed-garden' is a functioning, low-dependency app, a package supporting a monorepo of workspaces that utilize/depend on 'Three, WebGL, React, React Three Fiber, NextJS, TypeScript, + Apollo GraphQL', in active development.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marty-mcgee/threed-garden.git"
  },
  "author": {
    "name": "Marty McGee",
    "email": "mcgee.marty@gmail.com",
    "url": "https://github.com/marty-mcgee"
  },
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/marty-mcgee/threed-garden/issues/"
  },
  "homepage": "https://github.com/marty-mcgee/threed-garden/#readme",
  "private": false,
  "scripts": {
    "x:dev":                  "pnpm mm:next",
    "x:build":                "pnpm mm:build:next",
    "x:preview":              "pnpm mm:preview:next",
    "x:deploy":               "pnpm mm:deploy:github",
    "______________0":      "____ custom scripts _____________________________________",
    "mm":                   "next -p 3333",
    "mm:next":              "next -p 3333",
    "mm:vite":              "vite --port 4444",
    "mm:build:next":        "next export",
    "mm:build:vite":        "tsc && vite build",
    "mm:build:vue":         "vue-tsc --noEmit && vite build",
    "mm:preview":           "vite build && vite preview",
    "mm:start:node":        "node index.js",
    "mm:install":           "npm install --no-optional",
    "mm:install:npm":       "npm install",
    "mm:install:pnpm":      "pnpm i --no-optional",
    "mm:scrub:pnpm":        "rd /S /Q .\\node_modules & del /F .\\pnpm-lock.yaml",
    "mm:scrub:next":        "rd /S /Q .\\.next",
    "mm:test":              "tsc",
    "mm:jest":              "jest",
    "mm:deploy:github":     "echo 'deploy to?'",
    "______________1":      "____ ___________________________________________________",
    "______________X":      "____ ___________________________________________________"
  },
  "workspaces": {
    "packages": [
      "packages/common",
      "packages/solidity-ts",
      "packages/vite-app-ts",
      "packages/next-app-ts",
      "packages/subgraph",
      "packages/services/*",
      "packages/*/anyothers"
    ]
  },
  "packageManager": "yarn@3.2.1",
  "dependencies": {
    "@react-three/cannon": "^6.4.0",
    "@react-three/drei": "^9.28.2",
    "@react-three/fiber": "^8.7.0",
    "@react-three/flex": "^1.0.0",
    "@react-three/gltfjsx": "^4.3.4",
    "@react-three/postprocessing": "^2.6.2",
    "@react-three/xr": "^5.0.0",
    "@tweenjs/tween.js": "^18.6.4",
    "@udecode/zustood": "^1.1.1",
    "@use-gesture/react": "^10.2.19",
    "apollo-server": "^3.10.2",
    "konva": "^8.3.12",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "glsl-random": "^0.0.5",
    "glslify": "^7.1.1",
    "glslify-loader": "^2.0.0",
    "next": "^12.2.5",
    "next-pwa": "^5.6.0",
    "paper": "^0.12.15",
    "postprocessing": "^6.28.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-konva": "^18.2.1",
    "react-native": "^0.70.0",
    "react-spring": "^9.5.2",
    "react-zdog": "^1.0.11",
    "scheduler": ">=0.19.0",
    "three": "^0.144.0",
    "three-stdlib": "^2.12.1",
    "uuid": "^9.0.0",
    "vue": "^3.2.38",
    "web-vitals": "^0.2.4",
    "webgl": "^0.0.7",
    "webgl-detector": "^1.1.11",
    "zdog": "^1.1.3",
    "zustand": "^4.1.1"
  },
  "optionalDependencies": {
    "chalk": "^4.1.2",
    "commander": "^9.4.0",
    "cross-env": "^7.0.3",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier-plugin-solidity": "^1.0.0-dev.23",
    "shx": "^0.3.4",
    "syncpack": "^8.2.4"
  },
  "devDependencies": {
    "@babel/core": "@>=7.0.0 <8.0.0",
    "@babel/preset-env": "^7.19.0",
    "@next/bundle-analyzer": "^12.2.5",
    "@types/dat.gui": "^0.7.7",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.7.15",
    "@types/express": "^4.17.13",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@types/three": "^0.144.0",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-react": "^2.0.1",
    "@vitejs/plugin-vue": "^3.0.3",
    "eslint": "^8.23.1",
    "file-loader": "^6.2.0",
    "jest": "^29.0.2",
    "next-transpile-modules": "^9.0.0",
    "path": "^0.12.7",
    "pnpm": "^7.10.0",
    "prettier": "^2.7.1",
    "raw-loader": "^4.0.2",
    "ts-loader": "^9.2.5",
    "ts-node":"^10.9.1",
    "typescript": "^4.8.2",
    "url-loader": "^4.1.1",
    "vite": "^3.0.9",
    "vue-tsc": "^0.40.7",
    "webpack": "@>=5.9.0 <6.0.0",
    "wordpress-plugin": "^0.1.8-alpha"
  },
  "engines" : {
    "npm": ">=7.0.0",
    "node": ">=16.0.0"
  },
  "lint-staged": {
    "packages/vite-app-ts/src/**/*.{ts,tsx}": [
      "yarn workspace @scaffold-eth/vite-app lint --fix",
      "yarn workspace @scaffold-eth/vite-app format"
    ],
    "packages/nextjs-app-ts/src/**/*.{ts,tsx}": [
      "yarn workspace @scaffold-eth/nextjs-app lint --fix",
      "yarn workspace @scaffold-eth/nextjs-app format"
    ],
    "packages/threed-app-ts/src/**/*.{ts,tsx}": [
      "yarn workspace @scaffold-eth/threed-app lint --fix",
      "yarn workspace @scaffold-eth/threed-app format"
    ],
    "packages/common/src/**/*.{ts,tsx}": [
      "yarn workspace @scaffold-eth/common lint --fix",
      "yarn workspace @scaffold-eth/common format"
    ],
    "packages/commands/src/**/*.{ts,tsx}": [
      "yarn workspace @scaffold-eth/commands lint --fix",
      "yarn workspace @scaffold-eth/commands format"
    ],
    "packages/solidity-ts/tests/**/*.ts": [
      "yarn workspace @scaffold-eth/solidity lint --fix",
      "yarn workspace @scaffold-eth/solidity format"
    ],
    "packages/solidity-ts/helpers/**/*.ts": [
      "yarn workspace @scaffold-eth/solidity lint --fix",
      "yarn workspace @scaffold-eth/solidity format"
    ],
    "packages/solidity-ts/tasks/**/*.ts": [
      "yarn workspace @scaffold-eth/solidity lint --fix",
      "yarn workspace @scaffold-eth/solidity format"
    ],
    "packages/solidity-ts/deploy/**/*.ts": [
      "yarn workspace @scaffold-eth/solidity lint --fix",
      "yarn workspace @scaffold-eth/solidity format"
    ],
    "packages/solidity-ts/**/*.sol": [
      "yarn workspace @scaffold-eth/solidity format",
      "yarn workspace @scaffold-eth/solidity solhint --fix"
    ]
  },
  "volta": {
    "node": "16.15.0"
  },
  "keywords": [
    "javascript",
    "ecmascript",
    "typescript",
    "workspaces",
    "vite",
    "react",
