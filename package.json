{
  "name": "threed-garden",
  "version": "0.11.1",
  "description": "ThreeD Garden on ThreeJS React-Fiber using Vite/Vue/NextJS and Farmbot Ruby/JS (available as a standalone app and a WordPress plugin). 'threed-garden' is a functioning, low-dependency app, a package supporting a monorepo of workspaces that utilize/depend on 'ThreeJS, WebGL, React, Vue, TypeScript <Fiber>, GraphQL', in active development.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marty-mcgee/threed-garden.git"
  },
  "author": {
    "name": "Marty McGee",
    "email": "mcgee.marty@gmail.com",
    "url": "https://github.com/marty-mcgee"
  },
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/marty-mcgee/threed-garden/issues/"
  },
  "homepage": "https://github.com/marty-mcgee/threed-garden/#readme",
  "private": false,
  "workspaces": {
    "packages": [
      "packages/_sandbox",
      "packages/open-juice",
      "packages/react-garden",
      "packages/vite-garden",
      "packages/vue-garden",
      "packages/wordpress-plugin"
    ]
  },
  "scripts": {
    "______________0":      "____ custom scripts _____________________________________",
    "mm":                   "next -p 3333",
    "mm:next":              "next -p 3333",
    "mm:vite":              "vite --port 4444",
    "mm:dev":               "vite --port 5555",
    "mm:build":             "tsc && vite build",
    "mm:build:vue":         "vue-tsc --noEmit && vite build",
    "mm:next:export":       "next export",
    "mm:preview":           "vite preview",
    "mm:start":             "node index.js",
    "mm:install":           "npm install --no-optional",
    "mm:install:npm":       "npm install",
    "mm:install:pnpm":      "pnpm i --no-optional",
    "mm:scrub:pnpm":        "rd /S /Q .\\node_modules & del /F .\\pnpm-lock.yaml",
    "mm:scrub:next":        "rd /S /Q .\\.next",
    "mm:test":              "tsc",
    "mm:jest":              "jest",
    "______________1":      "____ you can use these too ______________________________",
    "build":                "pnpm mm:build",
    "react-garden:mm":      "yarn workspace react-garden mm",
    "react-garden:tsc":     "yarn workspace react-garden tsc",
    "react-garden:dev":     "yarn workspace react-garden dev",
    "react-garden:build":   "yarn workspace react-garden build",
    "react-garden:test":    "yarn workspace react-garden test",
    "react-garden:jest":    "yarn workspace react-garden jest",
    "vue-garden:mm":        "yarn workspace vue-garden mm",
    "vue-garden:tsc":       "yarn workspace vue-garden tsc",
    "vue-garden:dev":       "yarn workspace vue-garden dev",
    "vue-garden:build":     "yarn workspace vue-garden build",
    "vue-garden:test":      "yarn workspace vue-garden test",
    "vue-garden:jest":      "yarn workspace vue-garden jest",
    "vite-garden:mm":       "yarn workspace vite-garden mm",
    "vite-garden:build":    "yarn workspace vite-garden build",
    "vite-garden:dev":      "yarn workspace vite-garden dev",
    "vite-garden:preview":  "yarn workspace vite-garden preview",
    "______________X":      "____ ___________________________________________________"
  },
