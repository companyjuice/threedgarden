{
  "name": "threed-garden",
  "version": "0.10.9",
  "description": "ThreeD Garden on ThreeJS React-Fiber using Vite/NextJS and Farmbot Ruby/JS (available as a standalone app and a WordPress plugin). 'threed-garden' is a functioning, low-dependency app, a package supporting a monorepo of workspaces that utilize/depend on 'ThreeJS, WebGL, React, Vue, TypeScript <Fiber>, GraphQL', in active development.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marty-mcgee/threed-garden.git"
  },
  "author": {
    "name": "Marty McGee",
    "email": "mcgee.marty@gmail.com",
    "url": "https://github.com/marty-mcgee"
  },
  "private": false,
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/marty-mcgee/threed-garden/issues/"
  },
  "homepage": "https://github.com/marty-mcgee/threed-garden/#readme",
  "workspaces": {
    "packages": [
      "packages/_sandbox",
      "packages/@threed/hardhat",
      "packages/@threed/subgraph",
      "packages/@threed/react-dapp",
      "packages/nuxt-garden",
      "packages/react-garden",
      "packages/open-juice",
      "packages/vite-garden",
      "packages/wordpress-plugin"
    ],
    "nohoist": [
      "**/@graphprotocol/graph-ts",
      "**/@graphprotocol/graph-ts/**",
      "**/hardhat",
      "**/hardhat/**",
      "**/hardhat-ts",
      "**/hardhat-ts/**"
    ]
  },
  "type": "module",
  "scripts": {
    "____________0": "___ custom scripts ___________________________________",
    "mm":                 "pnpm mm:vite",
    "mm:start":           "node index.js",
    "mm:vite":            "vite",
    "mm:dev":             "vite --port 4445",
    "mm:build":           "tsc && vite build",
    "mm:build:vue":       "vue-tsc --noEmit && vite build",
    "mm:preview":         "vite preview --port 4446",
    "mm:install":         "npm install --no-optional",
    "mm:install:npm":     "npm install",
    "mm:install:pnpm":    "pnpm i --no-optional",
    "mm:scrub":           "rd /S /Q .\\node_modules & rd /S /Q .\\.next & del /F .\\pnpm-lock.yaml ;",
    "mm:test":            "tsc",
    "____________1": "___ you can use these too ____________________________",
    "build":              "pnpm react-garden:build",
    "react-garden:mm":    "yarn workspace react-garden mm",
    "react-garden:tsc":   "yarn workspace react-garden tsc",
    "react-garden:dev":   "yarn workspace react-garden dev",
    "react-garden:build": "yarn workspace react-garden build",
    "react-garden:test":  "yarn workspace react-garden test",
    "react-garden:jest":  "yarn workspace react-garden jest",
    "nuxt-garden:mm":     "yarn workspace nuxt-garden mm",
    "nuxt-garden:tsc":    "yarn workspace nuxt-garden tsc",
    "nuxt-garden:dev":    "yarn workspace nuxt-garden dev",
    "nuxt-garden:build":  "yarn workspace nuxt-garden build",
    "nuxt-garden:test":   "yarn workspace nuxt-garden test",
    "nuxt-garden:jest":   "yarn workspace nuxt-garden jest",
    "vite-garden:mm":     "yarn workspace vite-garden mm",
    "vite-garden:build":  "yarn workspace vite-garden build",
    "vite-garden:dev":    "yarn workspace vite-garden dev",
    "vite-garden:preview": "yarn workspace vite-garden preview",
    "vue-dapp:build":     "yarn workspace @threed/vue-dapp build",
    "vue-dapp:ipfs":      "yarn workspace @threed/vue-dapp ipfs",
    "vue-dapp:lint":      "yarn workspace @threed/vue-dapp lint",
    "vue-dapp:serve":     "yarn workspace @threed/vue-dapp serve",
    "scaffold:mm":        "echo 'USE scaffold:mm'",
    "scaffold:build":     "echo 'USE scaffold:build'",
    "hardhat:chain":      "yarn workspace @threed/hardhat chain",
    "hardhat:fork":       "yarn workspace @threed/hardhat fork",
    "hardhat:node":       "yarn workspace @threed/hardhat chain",
    "hardhat:test":       "yarn workspace @threed/hardhat test",
    "hardhat:compile":    "yarn workspace @threed/hardhat compile",
    "hardhat:deploy":     "yarn workspace @threed/hardhat deploy",
    "hardhat:watch":      "yarn workspace @threed/hardhat watch",
    "hardhat:accounts":   "yarn workspace @threed/hardhat accounts",
    "hardhat:verify":     "yarn workspace @threed/hardhat verify",
    "hardhat:balance":    "yarn workspace @threed/hardhat balance",
    "hardhat:send":       "yarn workspace @threed/hardhat send",
    "hardhat:generate":   "yarn workspace @threed/hardhat generate",
    "hardhat:account":    "yarn workspace @threed/hardhat account",
    "hardhat:mineContractAddress":  "cd packages/hardhat && npx hardhat mineContractAddress",
    "hardhat:wallet":               "cd packages/hardhat && npx hardhat wallet",
    "hardhat:fundedwallet":         "cd packages/hardhat && npx hardhat fundedwallet",
    "hardhat:flatten":              "cd packages/hardhat && npx hardhat flatten",
    "hardhat:clean":                "cd packages/hardhat && npx hardhat clean",
    "hardhat:mint":           "yarn workspace @threed/hardhat mint",
    "react-dapp:ipfs":        "yarn workspace @threed/react-dapp ipfs",
    "react-dapp:surge":       "yarn workspace @threed/react-dapp surge",
    "react-dapp:s3":          "yarn workspace @threed/react-dapp s3",
    "react-dapp:ship":        "yarn workspace @threed/react-dapp ship",
    "react-dapp:build":       "yarn workspace @threed/react-dapp build --max-old-space-size=12288",
    "react-dapp:start":       "yarn workspace @threed/react-dapp start",
    "react-dapp:eject":       "yarn workspace @threed/react-dapp eject",
    "react-dapp:test":        "yarn workspace @threed/react-dapp test",
    "react-dapp:prettier":    "yarn workspace @threed/react-dapp prettier",
    "react-dapp:theme":       "yarn workspace @threed/react-dapp theme",
    "react-dapp:watch-theme": "yarn workspace @threed/react-dapp watch",
    "services:run-graph-node":    "yarn workspace @threed/services run-graph-node",
    "services:remove-graph-node": "yarn workspace @threed/services remove-graph-node",
    "services:clean-graph-node":  "yarn workspace @threed/services clean-graph-node",
    "subgraph:auth":          "yarn workspace @threed/subgraph auth",
    "subgraph:codegen":       "yarn workspace @threed/subgraph codegen",
    "subgraph:build":         "yarn workspace @threed/subgraph build",
    "subgraph:deploy":        "yarn workspace @threed/subgraph deploy",
    "subgraph:graph-prepare": "mustache packages/subgraph/config/config.json packages/subgraph/src/subgraph.template.yaml > packages/subgraph/subgraph.yaml",
    "subgraph:graph-codegen": "yarn workspace @threed/subgraph graph codegen",
    "subgraph:graph-build":   "yarn workspace @threed/subgraph graph build",
    "subgraph:graph-create-local": "yarn workspace @threed/subgraph graph create --node http://localhost:8020/ scaffold-eth-ts/your-contract",
    "subgraph:graph-remove-local": "yarn workspace @threed/subgraph graph remove --node http://localhost:8020/ scaffold-eth-ts/your-contract",
    "subgraph:graph-deploy-local": "yarn workspace @threed/subgraph graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 scaffold-eth-ts/your-contract",
    "subgraph:graph-ship-local": "yarn graph-prepare && yarn graph-codegen && yarn graph-deploy-local",
    "subgraph:deploy-and-graph": "yarn deploy && yarn graph-ship-local",
    "______________X": "_____________________________________________________"
  },
  "devDependencies": {
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.1",
    "@vitejs/plugin-vue": "^3.0.3",
    "pnpm": "^7.10.0",
    "typescript": "^4.8.2",
    "vite": "^3.0.9",
    "vue-tsc": "^0.40.7",
    "yarn": "^1.22.19"
  },
  "dependencies": {
    "@react-three/fiber": "^8.7.0",
    "apollo-server": "^3.10.2",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "three": "^0.144.0",
    "vue": "^3.2.38",
    "webgl": "^0.0.7"
  },
  "engines" : { 
    "npm" : ">=7.0.0",
    "node" : ">=16.0.0"
  },
  "keywords": [
    "javascript",
    "ecmascript",
    "typescript",
    "workspaces",
    "vite",
    "react",
    "nextjs",
    "vue",
    "nuxt",
    "mui",
    "ethereum",
    "threed",
    "threedgarden",
    "threedgarden.eth",
    "farmbot",
    "farmbot.eth",
    "https://github.com/marty-mcgee/threed",
    "https://github.com/marty-mcgee/threed-garden",
    "https://threedgarden.com/demo",
    "https://garden.university",
    "https://threed.ai"
  ]
}
