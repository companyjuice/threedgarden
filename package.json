    "paper": "^0.12.15",
    "postprocessing": "^6.28.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-konva": "^18.2.1",
    "react-native": "^0.70.0",
    "react-spring": "^9.5.2",
    "react-zdog": "^1.0.11",
    "scheduler": ">=0.19.0",
